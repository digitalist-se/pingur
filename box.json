{
  "chmod": "0755",
  "directories": [
    "src"
  ],
  "files": [
    "app/pingur",
    "app/config/services.yml",
    "vendor/autoload.php"
  ],
  "finder": [
    {
      "in": "vendor",
      "name": "*.php",
      "exclude": [
        "phpunit",
        "Tests",
        "tests",
        "test"
      ]
    }
  ],
  "git-version": "package_version",
  "intercept": true,
  "main": "app/pingur",
  "output": "pingur.phar",
  "stub": true
}

